{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "outputDirectory": "dist",
  "includeFiles": [
    "image/**/*",
    "*.html",
    "*.js",
    "*.css"
  ],
  "routes": [
    {
      "src": "/gallery-items.html",
      "dest": "/gallery-items.html"
    },
    {
      "src": "/image/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
