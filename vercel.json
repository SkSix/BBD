{
  "version": 2,
  "builds": [
    {
      "src": "generate-gallery.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/gallery-items.html",
      "dest": "/gallery-items.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
